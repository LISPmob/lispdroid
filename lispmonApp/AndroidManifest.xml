<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.le.lispmon"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="8" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".lispMonitor"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".logActivity"></activity>
        <activity android:name="mapCacheActivity"></activity>
        <activity android:name="confActivity"></activity>
        <activity android:name="pingActivity"></activity>

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    
</manifest>